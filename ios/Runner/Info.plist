<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>update_location_worker_task_name</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Shojag</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Shojag</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>tel</string>
	</array>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSCameraUsageDescription</key>
	<string>Shojag needs access to the camera to take photos and videos</string>
	<key>NSContactsUsageDescription</key>
    <string>Shojag needs access to your contacts to help you invite and connect with friends and family.</string>

	<key>NSLocationUsageDescription</key>
    <string>Shojag needs access to your location to provide real-time location-based alerts and to notify your friends and family when you reach your predefined locations</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Shojag requires access to your location at all times and when in use to provide real-time location-based alerts and to notify your friends and family when you reach your predefined locations, ensuring safety and efficient tracking.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Shojag needs access to your location at all times to provide accurate location tracking for your alerts and notifications, even when the app is in the background.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Shojag uses your location when the app is in use to notify your friends and family when you arrive at specific locations like home, work, or custom places, ensuring better safety and coordination.</string>
	<key>NSMicrophoneUsageDescription</key>
    <string>Shojag needs access to the microphone to capture sounds.</string>
	<key>NSPhotoLibraryUsageDescription</key>
    <string>Shojag needs access to the photo library to select and upload photos and videos.</string>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>fetch</string>
		<string>processing</string>
		<string>location</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIStatusBarHidden</key>
	<true/>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
</dict>
</plist>
